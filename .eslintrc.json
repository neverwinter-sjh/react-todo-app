{    
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {      
      "no-unused-vars": "off",
      "no-undef": "off",
      "react/prop-types": 0,
      "react/forbid-prop-types": 0
    }
}
